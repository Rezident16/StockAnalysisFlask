{
    "version": 2,
    "builds": [
      {
        "src": "app/__init__.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/__init__.py"
      }
    ],
    "build": {
      "env": {
        "PIP_NO_CACHE_DIR": "off"
      },
      "commands": [
        "sh install_talib.sh",
        "pip install -r requirements.txt"
      ]
    }
  }
