{
    "version": 2,
    "builds": [
      {
        "src": "app/__init__.py",
        "use": "@vercel/python",
        "config": {
          "buildCommand": "sh build.sh"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/__init__.py"
      }
    ],
    "env": {
      "PIP_NO_CACHE_DIR": "off"
    }
  }
